pipeline {
    agent any
    stages {
        stage('Clone') {
            steps {
                print "Clone"
            }
        }
        stage('Build') {
            steps {
                print "Build"
            }
        }
        stage('Deploy') {
            steps {
                print "Deploy Image"
            }
        }
        stage('Test') {
            steps {
                print "Testing"
            }
        }
    }
}